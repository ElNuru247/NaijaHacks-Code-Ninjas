<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tech.codeninjas.vetfinder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DiagnoseActivity" />
        <activity android:name=".PendingTreatmentActivity" />
        <activity android:name=".VetListingActivity" />
        <activity android:name=".QADiagnoseActivity" />
        <activity android:name=".PhotoDiagnoseActivity" />
        <activity android:name=".DiagnosesResultActivity" />
        <activity android:name=".VetProfileActivity"></activity>
    </application>

</manifest>